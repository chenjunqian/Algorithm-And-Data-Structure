#ifndef LIST_
#define LIST_

#include <stdlib.h>

typedef struct ListElmt_{
	void *data;
	struct ListElmt_ *next;
}ListElmt;

typedef struct List_{
	int size;
	int (*match)(const void *key1,const void *key2);
	void (*destory)(void *data);
};

